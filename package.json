{
  "name": "floyo-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Floyo - File usage pattern tracking and integration suggestions",
  "type": "module",
  "engines": {
    "node": ">=20 <21"
  },
  "scripts": {
    "ops": "tsx ops/cli.ts",
    "ops:doctor": "npm run ops doctor",
    "ops:init": "npm run ops init",
    "ops:check": "npm run ops check",
    "ops:release": "npm run ops release",
    "ops:snapshot": "npm run ops snapshot",
    "ops:restore": "npm run ops restore",
    "ops:rotate-secrets": "npm run ops rotate-secrets",
    "ops:sb-guard": "npm run ops sb-guard",
    "ops:test:e2e": "npm run ops test:e2e",
    "ops:benchmark": "npm run ops benchmark",
    "ops:lintfix": "npm run ops lintfix",
    "ops:docs": "npm run ops docs",
    "ops:changelog": "npm run ops changelog",
    "ops:orchestrate": "npm run ops orchestrate",
    "ops:aurora-prime": "npm run ops aurora-prime",
    "aurora-prime": "tsx scripts/aurora-prime.ts",
    "master-omega-prime": "tsx scripts/master-omega-prime.ts",
    "master-omega-prime:fix": "tsx scripts/master-omega-prime.ts --fix",
    "wiring:run": "tsx tools/wiring/harness.ts",
    "wiring:doctor": "tsx tools/wiring/wire_doctor.ts",
    "wiring:report": "node -e \"console.log(JSON.stringify(require('./reports/connectivity/connectivity.json'), null, 2))\"",
    "agent:run": "tsx agent-engine/cli.ts",
    "agent:test": "tsx agent-engine/cli.ts",
    "unified-agent:run": "tsx unified-agent/cli.ts run",
    "unified-agent:status": "tsx unified-agent/cli.ts status",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "ci": "npm run ci:backend && npm run ci:frontend",
    "ci:backend": "cd backend && ruff check . && black --check . && mypy backend/ && pytest tests/unit/ -v",
    "ci:frontend": "cd frontend && npm run lint && npm run type-check && npm test && npm run build",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:coverage": "jest --coverage",
    "build": "npm run build --workspace=frontend",
    "demo:privacy": "tsx scripts/demo-privacy.ts",
    "security:check": "tsx scripts/security-self-check.ts",
    "verify:rls": "tsx scripts/verify-rls.ts",
    "audit:secrets": "tsx scripts/audit-secrets.ts",
    "migrate:concurrent": "bash scripts/migrate-concurrent.sh",
    "start-ci": "cd frontend && PORT=3000 next start -p 3000",
    "lhci": "lhci autorun --config=./lighthouserc.json || true",
    "a11y": "pa11y-ci",
    "vercel:login": "vercel login || true",
    "vercel:pull": "cd frontend && vercel pull --yes --environment=preview --token $VERCEL_TOKEN",
    "vercel:build": "cd frontend && vercel build --token $VERCEL_TOKEN",
    "vercel:deploy:preview": "cd frontend && vercel deploy --prebuilt --token $VERCEL_TOKEN > ../.vercel-url.txt",
    "vercel:deploy:prod": "cd frontend && vercel deploy --prebuilt --prod --token $VERCEL_TOKEN",
    "supa:login": "supabase login --token $SUPABASE_ACCESS_TOKEN",
    "supa:link": "supabase link --project-ref $SUPABASE_PROJECT_REF",
    "supa:migrate:plan": "supabase db changes commit --use-migra",
    "supa:migrate:apply": "supabase db push --local false",
    "supa:status": "supabase db remote commit --dry-run || true",
    "typecheck": "npm run type-check",
    "lint:all": "./scripts/lint-all.sh",
    "lint:unused": "eslint . --report-unused-disable-directives --format json > reports/eslint-unused-disables.json || true",
    "prune:exports": "ts-prune > reports/ts-prune.txt || true",
    "audit:deps": "depcheck --json > reports/depcheck.json || true",
    "scan:usage": "knip --reporter json > reports/knip.json || true",
    "hygiene": "npm run typecheck && npm run lint && npm run prune:exports && npm run scan:usage && npm run audit:deps",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" && cd backend && black . && ruff format .",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\" && cd backend && black --check && ruff format --check .",
    "bench": "tsx bench/example.bench.ts",
    "bench:trend": "node scripts/bench-trend.js",
    "openapi:generate": "tsx scripts/generate-openapi.ts",
    "env:validate": "tsx -e \"import { validatePublicEnv } from './frontend/lib/env'; validatePublicEnv(); console.log('âœ… Environment variables validated')\"",
    "api:audit": "tsx scripts/audit-api-endpoints.ts",
    "migrations:align": "tsx scripts/align-migrations.ts",
    "schema:validate": "tsx scripts/validate-schema-alignment.ts",
    "generate-sample-data": "tsx scripts/generate-sample-data.ts",
    "metrics:aggregate:daily": "python -m backend.jobs.metrics_aggregation --daily",
    "metrics:aggregate:weekly": "python -m backend.jobs.metrics_aggregation --weekly",
    "metrics:report:weekly": "python -m backend.jobs.weekly_metrics_report",
    "retention:analyze": "python -m backend.jobs.retention_analysis",
    "load-test": "bash scripts/run-load-tests.sh",
    "schedule-metrics-jobs": "bash scripts/schedule-metrics-jobs.sh",
    "verify-sprint-blockers": "tsx scripts/verify-sprint-blockers.ts"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.0",
    "@prisma/client": "^5.7.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.8.0",
    "@jest/globals": "^29.7.0",
    "@lhci/cli": "^0.13.0",
    "@playwright/test": "^1.40.0",
    "@sentry/nextjs": "^7.91.0",
    "@supabase/supabase-js": "^2.81.1",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "depcheck": "^1.4.7",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "knip": "^5.68.0",
    "madge": "^6.1.0",
    "pa11y-ci": "^3.1.0",
    "pg": "^8.16.3",
    "prisma": "^5.7.0",
    "supabase": "^2.5.0",
    "ts-prune": "^0.10.3",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vercel": "^39.0.0",
    "wait-on": "^7.2.0"
  }
}
